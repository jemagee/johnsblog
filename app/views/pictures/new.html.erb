<%if @picture.errors.any? %>
  <% @picture.errors.full_messages.each do |msg| %>
    <p>
      <%= msg %>
    </p>
  <% end %>
<% end %>

<%= form_for(@picture) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :image %>
  <%= f.file_field :image %>


  <%= f.submit "Upload Picture" %>

<% end %>